		{
			"name": "[concat(parameters('factoryName'), '/windowtransfer')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "windowtransferin",
								"type": "DatasetReference"
							},
							"name": "transferwindow"
						}
					],
					"sinks": [
